//Print Adjancey List

class Adjacency 
{
    static void printGraph(ArrayList<ArrayList<Integer>> list, int v)
	{
	    for(int i=0;i<v;i++)
	    {
	        System.out.print(i);
	        
	        for(int j=0;j<list.get(i).size();j++)
	        {
	            System.out.print("-> "+list.get(i).get(j));
	        }
	        System.out.println();
	    }
	}
}
